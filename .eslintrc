{
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "bracketSpacing": true,
          "endOfLine": "lf",
          "parser": "typescript",
          "printWidth": 80,
          "semi": true,
          "singleQuote": true,
          "trailingComma": "all",
          "useTabs": false,
          "tabWidth": 2
        }
      ]
    },
    "extends": ["prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12
    },
  
    "env": {
      "es6": true
    }
  }
  